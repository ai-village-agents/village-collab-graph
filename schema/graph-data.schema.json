{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/schema/graph-data.schema.json",
  "title": "Agent Village Collaboration Graph",
  "type": "object",
  "additionalProperties": false,
  "required": ["metadata", "nodes", "links"],
  "properties": {
    "metadata": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "title",
        "description",
        "total_days",
        "total_events",
        "total_agents",
        "total_links",
        "total_collaborations",
        "generated",
        "generated_by",
        "source",
        "normalization"
      ],
      "properties": {
        "title": {
          "type": "string",
          "minLength": 1
        },
        "description": {
          "type": "string",
          "minLength": 1
        },
        "total_days": {
          "type": "integer",
          "minimum": 1
        },
        "total_events": {
          "type": "integer",
          "minimum": 1
        },
        "total_agents": {
          "type": "integer",
          "minimum": 1
        },
        "total_links": {
          "type": "integer",
          "minimum": 0
        },
        "total_collaborations": {
          "type": "integer",
          "minimum": 0
        },
        "generated": {
          "type": "string",
          "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
        },
        "generated_by": {
          "type": "string",
          "minLength": 1
        },
        "source": {
          "type": "string",
          "minLength": 1
        },
        "normalization": {
          "type": "string",
          "minLength": 1
        }
      }
    },
    "nodes": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["id", "events", "family"],
        "properties": {
          "id": {
            "type": "string",
            "minLength": 1
          },
          "events": {
            "type": "integer",
            "minimum": 1
          },
          "family": {
            "type": "string",
            "enum": ["Claude", "DeepSeek", "GPT", "Gemini", "Grok", "o-series"]
          }
        }
      }
    },
    "links": {
      "type": "array",
      "minItems": 0,
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["source", "target", "weight"],
        "properties": {
          "source": {
            "type": "string",
            "minLength": 1
          },
          "target": {
            "type": "string",
            "minLength": 1
          },
          "weight": {
            "type": "integer",
            "minimum": 1
          }
        }
      }
    }
  }
}
